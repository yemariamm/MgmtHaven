<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:onClick="linked">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/Home"
        android:id="@+id/textView62"
        android:layout_marginTop="69dp"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@+id/textView64"
        android:layout_toStartOf="@+id/textView64"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/ad"
        android:id="@+id/textView63"
        android:layout_below="@+id/textView62"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/adt"
        android:id="@+id/textView64"
        android:layout_below="@+id/textView63"
        android:layout_alignLeft="@+id/textView63"
        android:layout_alignStart="@+id/textView63"
        android:textColor="#595757" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spinner3"
        android:layout_below="@+id/textView64"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="49dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/addr"
        android:id="@+id/textView65"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/spinner3"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/addres"
        android:id="@+id/textView66"
        android:layout_below="@+id/textView65"
        android:layout_alignLeft="@+id/textView65"
        android:layout_alignStart="@+id/textView65"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/tenants"
        android:id="@+id/textView67"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/textView66"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/bill"
        android:id="@+id/textView68"
        android:layout_below="@+id/textView67"
        android:layout_alignLeft="@+id/textView64"
        android:layout_alignStart="@+id/textView64"
        android:layout_marginTop="18dp"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/linked"
        android:id="@+id/linked"
        android:layout_alignTop="@+id/textView68"
        android:layout_toRightOf="@+id/textView63"
        android:layout_toEndOf="@+id/textView63"
        android:clickable="true"
        android:onClick="linked"
        android:textColor="#595757" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/imageButton2"
        android:src="@drawable/arrowl"
        android:background="#caffffff"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/textView62"
        android:layout_alignStart="@+id/textView62" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/accts"
        android:id="@+id/textView70"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_toRightOf="@+id/textView68"
        android:layout_toEndOf="@+id/textView68"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/delete"
        android:id="@+id/textView78"
        android:layout_alignTop="@+id/linked"
        android:layout_alignRight="@+id/textView70"
        android:layout_alignEnd="@+id/textView70"
        android:textColor="#595757"
        android:onClick="delete" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/lisa"
        android:id="@+id/textView79"
        android:layout_below="@+id/textView68"
        android:layout_alignLeft="@+id/textView68"
        android:layout_alignStart="@+id/textView68"
        android:layout_marginTop="20dp"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/linked"
        android:id="@+id/linked2"
        android:layout_alignTop="@+id/textView79"
        android:layout_alignLeft="@+id/linked"
        android:layout_alignStart="@+id/linked"
        android:onClick="linked"
        android:textColor="#595757" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/delete"
        android:id="@+id/textView80"
        android:layout_alignTop="@+id/linked2"
        android:layout_alignLeft="@+id/textView78"
        android:layout_alignStart="@+id/textView78"
        android:textColor="#595757"
        android:onClick="delete" />

</RelativeLayout>
